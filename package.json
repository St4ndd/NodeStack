{
  "name": "nodestack",
  "version": "1.2.4",
  "bin": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "node server.js",
    "make-exe": "npm run build && pkg . --out-path build"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}